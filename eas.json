{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "resourceClass": "large",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "http://localhost:8081"
      }
    },
    "preview": {
      "distribution": "internal",
      "resourceClass": "large",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "https://auctionade.expo.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "resourceClass": "large",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "https://auctionade.expo.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

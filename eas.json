{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "resourceClass": "large",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "http://auctionade.onrender.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "resourceClass": "large",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "http://auctionade.onrender.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "resourceClass": "large",
      "env": {
        "EXPO_PUBLIC_BASE_URL": "http://auctionade.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
